{
    "manifest_version": 3,
    "name": "DPI & DNS Unblocker",
    "version": "1.3",
    "description": "Access blocked websites using Local Proxy and DNS tools.",
    "permissions": [
        "proxy",
        "storage",
        "tabs",
        "declarativeNetRequest"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Access Control"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "48": "icon.png"
    }
}